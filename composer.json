{
    "name": "thecodingmachine/yaco-service-provider",
    "type": "library",
    "description": "This package is a bridge between container-intop's service providers and YACO (the PSR-11 compliant container compiler).",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TheCodingMachine\\Yaco\\ServiceProvider\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TheCodingMachine\\Yaco\\ServiceProvider\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.0",
        "container-interop/service-provider": "~0.2.0",
        "thecodingmachine/yaco": "~0.4.0",
        "puli/discovery": "^1.0-beta9@beta"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/xhuberty/assembly"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "mnapoli/assembly": "dev-callable-service-providers",
        "satooshi/php-coveralls": "~1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
